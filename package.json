{
  "name": "toronto-rising-tts",
  "version": "1.0.0",
  "description": "Tabletop Simulator LUA modding project",
  "scripts": {
    "build:css": "sass UI/styles.scss UI/styles.css",
    "build:css:watch": "sass --watch UI/styles.scss:UI/styles.css",
    "build": "npm run build:css",
    "lint": "echo 'Install luacheck for linting: luarocks install luacheck'",
    "format": "echo 'Install stylua for formatting: cargo install stylua'"
  },
  "keywords": [
    "tabletop-simulator",
    "lua",
    "modding"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "sass": "^1.77.0"
  }
}
